{
	"name": "prompt-mixer-llamaindex-example-connector",
	"version": "1.0.2",
	"description": "Llamaindex Connector",
	"type": "module",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck  && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"lint": "eslint '**/*.{js,ts}' --quiet",
		"lint:fix": "eslint '**/*.{js,ts}' --fix",
		"format": "prettier '**/*.{js,ts,json,md}' --write",
		"format:check": "prettier '**/*.{js,ts,json,md}' --check",
		"check": "npm run lint && npm run format:check && npm run type:check",
		"type:check": "tsc --noEmit",
		"prepare": "husky"
	},
	"keywords": [
		"Connector",
		"Prompt Mixer",
		"Prompts",
		"LLM",
		"llamaindex"
	],
	"author": "Prompt Mixer",
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^24.1.0",
		"@typescript-eslint/eslint-plugin": "^8.37.0",
		"@typescript-eslint/parser": "^8.37.0",
		"builtin-modules": "5.0.0",
		"esbuild": "0.25.8",
		"eslint": "^9.31.0",
		"eslint-config-prettier": "^10.1.5",
		"eslint-plugin-prettier": "^5.5.3",
		"husky": "^9.1.7",
		"prettier": "^3.6.2",
		"tslib": "^2.8.1",
		"typescript": "^5.8.3"
	},
	"dependencies": {
		"llamaindex": "^0.11.19"
	}
}
